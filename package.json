{
  "name": "@cxing/moment-extension",
  "version": "0.1.1",
  "description": "Extending MomentJs with additional support and functionality",
  "keywords": [
    "moment",
    "momentjs",
    "date",
    "time",
    "calendar"
  ],
  "license": "MIT",
  "author": {
    "name": "Steve 'Cutter' Blades",
    "url": "https://cutterscrossing.com"
  },
  "contributors": [
    {
      "name": "Steve 'Cutter' Blades",
      "url": "https://cutterscrossing.com"
    }
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/moment-extension.umd.cjs",
  "module": "./dist/moment-extension.js",
  "exports": {
    ".": {
      "import": "./dist/moment-extension.js",
      "require": "./dist/moment-extension.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cutterbl/moment-extension.git"
  },
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "build": "vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "release": "commit-and-tag-version",
    "shrinkwrap": "zx ./.scripts/shrinkwrap.mjs"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@vitest/coverage-v8": "^1.6.0",
    "commit-and-tag-version": "^12.4.1",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "prettier": "^3.3.2",
    "terser": "^5.31.1",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "zx": "^8.1.2"
  },
  "dependencies": {
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45"
  }
}
